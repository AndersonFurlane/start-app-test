<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="app.start.test.com.br.startappteste.InterstitialsActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AUTOMATIC"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="interstitialsAutomatic"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FULLPAGE"
        android:id="@+id/button_splash"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button"
        android:onClick="interstitialsFullPage" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OFFERWALL"
        android:id="@+id/button3"
        android:layout_below="@+id/button_splash"
        android:layout_alignStart="@+id/button_splash"
        android:layout_marginTop="45dp"
        android:onClick="interstitialsOfferWall" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OVERLAY"
        android:id="@+id/button4"
        android:layout_below="@+id/button3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="63dp"
        android:onClick="interstitialsOverlay" />
</RelativeLayout>
